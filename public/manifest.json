{
  "manifest_version":2,
  "version":"1.0",
  "short_name": "Jimmy Aja Automation",
  "name": "Automating daily operations on jimmy aja",
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background":{
    "scripts":[
      "jquery-3.5.1.min.js",
      "moment.min.js",
      "background.js"
    ]
  },
  "content_scripts":[{
    "matches":["<all_urls>"],
    "all_frames": false,
    "js":[
      "./jquery-3.5.1.min.js",
      "./moment.min.js",
      "./content.js"
    ],
    "run_at":"document_end"
  }],
  "permissions":[
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'sefl'",
  "web_accessible_resources":[
    "index.html",
    "/static/*"
  ]
}
