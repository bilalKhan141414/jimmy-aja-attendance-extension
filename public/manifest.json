{
  "manifest_version":2,
  "version":"2.0",
  "short_name": "Jimmy Aja Automation",
  "name": "Automating daily operations on jimmy aja",
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "background":{
    "scripts":[
      "jquery-3.5.1.min.js",
      "moment.min.js",
      "background.js"
    ]
  },
  "content_scripts":[{
    "matches":["<all_urls>"],
    "all_frames": false,
    "js":[
      "./jquery-3.5.1.min.js",
      "./moment.min.js",
      "./content.js"
    ],
    "run_at":"document_end"
  }],
  
  "permissions":[
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'sefl'",
  "web_accessible_resources":[
    "index.html",
    "/static/*"
  ]
}
